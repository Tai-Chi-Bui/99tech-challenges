generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Animal {
  id          Int      @id @default(autoincrement())
  name        String
  species     String
  breed       String?
  age         Int?
  size        String?
  location    String?
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
